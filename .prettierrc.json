{
  "singleQuote": true,
  "printWidth": 120,
  "endOfLine": "auto",
  "semi": false,
  "tabWidth": 2,
  "bracketSpacing": false,
  "trailingComma": "all",
  "overrides": [
    {"files": ["*.json", ".prettierrc"], "options": {"parser": "json"}},
    {"files": "*.md", "options": {"parser": "markdown"}}
  ]
}
